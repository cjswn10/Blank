<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="favorite">

  <select id="listFavorite" resultType="favoriteVo" parameterType="java.lang.Integer">
   	SELECT m.id,f.fno FROM favorites f, member m WHERE f.mno=#{mno} AND f.fmno = m.mno
  </select>
   
  <delete id="deleteFavorite">
  	DELETE FROM favorites WHERE fno=#{fno}
  </delete>
 
</mapper>